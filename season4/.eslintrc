{ 
  "extends": ["react-native-wcandillon"],
  "plugins": [ "ts-exports", "reanimated"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "ts-exports/unused-exports": 2,
    "reanimated/js-function-in-worklet": 0,
    "reanimated/unsupported-syntax": 0,
    "reanimated/no-multiple-animated-style-usages": 0
  }
} 